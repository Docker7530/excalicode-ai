# 环境配置模板（复制为 .env 并填入真实值）

# MySQL
DB_HOST=localhost
DB_PORT=3306
DB_NAME=excalicode_ai
DB_USERNAME=root
DB_PASSWORD=your_password_here

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# JWT
# 密钥长度必须 >= 256 bits (32 字节)
# 生成强随机密钥: openssl rand -base64 32
JWT_SECRET=your-jwt-secret-key-at-least-32-bytes-long
# Token 过期时间（毫秒）: 86400000 = 24小时, 3600000 = 1小时
JWT_EXPIRATION=86400000

# AI
SILICONFLOW_API_KEY=sk-xxxx
